<h1>Restaurants</h1>
<ul>
<% @restaurants.each do |restaurant| %>
  <li>
    <%= link_to restaurant.name, restaurant_path(restaurant) %> - <%= restaurant.address %> -  <%= restaurant.category %> [<%= restaurant.phone_number %>]
  </li>
<% end %>  
</ul>
<br>
<%= link_to 'Create a restaurant', new_restaurant_path %>